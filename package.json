{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "vitest run tests/accessibility",
    "test:visual": "playwright test tests/visual",
    "test:ci": "vitest run --coverage && playwright test && npm run test:cypress:ci && npm run test:robot:ci",
    "test:watch": "vitest",
    "test:cypress": "cypress open",
    "test:cypress:component": "cypress open --component",
    "test:cypress:e2e": "cypress open --e2e",
    "test:cypress:run": "cypress run",
    "test:cypress:component:run": "cypress run --component",
    "test:cypress:e2e:run": "cypress run --e2e",
    "test:cypress:ci": "cypress run --headless --browser chrome",
    "test:cypress:coverage": "cypress run --env coverage=true",
    "test:cypress:analyze": "npx tsx scripts/analyze-test-errors.ts",
    "test:cypress:run:analyze": "npm run test:cypress:run; npm run test:cypress:analyze",
    "test:robot": "robot tests/robot",
    "test:robot:ci": "robot --outputdir test-results/robot tests/robot",
    "test:robot:coverage": "robot --listener RobotFramework.Coverage tests/robot",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:cypress:run && npm run test:robot",
    "test:all:with-log": "npx tsx scripts/run-all-tests-simple.ts",
    "test:all:coverage": "npm run test:unit && npm run test:cypress:coverage && npm run test:robot:coverage",
    "coverage:merge": "npx nyc merge coverage coverage/merged.json",
    "coverage:report": "npx nyc report --reporter=html --reporter=text --reporter=lcov",
    "verify:mvp": "npx tsx scripts/verify-mvp.ts",
    "validate:system": "npx tsx scripts/validate-system.ts",
    "validate:dashboard": "npx tsx scripts/validate-system.ts --module=dashboard",
    "validate:moradores": "npx tsx scripts/validate-system.ts --module=moradores",
    "validate:agendamentos": "npx tsx scripts/validate-system.ts --module=agendamentos",
    "validate:relatorios": "npx tsx scripts/validate-system.ts --module=relatorios",
    "validate:funcionarios": "npx tsx scripts/validate-system.ts --module=funcionarios",
    "validate:backup": "npx tsx scripts/validate-system.ts --module=backup",
    "validate:suporte": "npx tsx scripts/validate-system.ts --module=suporte",
    "validate:ci": "npx tsx scripts/validate-system.ts --headless=true --fail-fast"
  },
